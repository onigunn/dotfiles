export PATH=$PATH:"$HOME/bin" 
alias git-prod-push="gco production && git noffmerge master && git p && gcm && git p"
alias git-stag-push="gco staging && git noffmerge master && git p && gcm && git p"
alias git-prev-push="gco preview && git noffmerge master && git p && gcm && git p"
alias git-onl-push="gco online && git noffmerge master && git p && gcm && git p"
